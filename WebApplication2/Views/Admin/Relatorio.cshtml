@model WebApplication2.Models.BancoModel

@{
    ViewData["Title"] = "Relatorio";
    Layout = "_LayoutAdmin";
}
<form asp-action="RelatorioImposto" >
    <div asp-validation-summary="ModelOnly" class="text-danger"></div>
    <div class="form-group">
            
        <label asp-for="DtInicio" class="control-label"></label>
        <input asp-for="DtInicio" class="form-control" />
        <span asp-validation-for="DtInicio" class="text-danger"></span>
            
    </div>
    <div class="form-group">
                
        <label asp-for="DtFim" class="control-label"></label>
        <input asp-for="DtFim" class="form-control" />
        <span asp-validation-for="DtFim" class="text-danger"></span>
                
    </div>
<div class="form-group">
                
        <label asp-for="Banco" class="control-label"></label>
        <input asp-for="Banco" class="form-control" />
        <span asp-validation-for="Banco" class="text-danger"></span>
                
    </div>
     
    <div class="form-group">
        <label class="control-label"></label>
    </div>
     

    <div class="form-group">
        <input type="submit" value="Gerar Relatorio" class="btn btn-primary"/>
    </div>
        
</form>